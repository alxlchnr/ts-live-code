{
  "name": "ts-dojo-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate project to quick start your typescript dojo",
  "main": "dist/main.js",
  "scripts": {
    "compile": "tsc",
    "compile:watch": "tsc -w",
    "dist:watch": "nodemon",
    "start": "concurrently --kill-others \"npm run compile:watch\" \"npm run dist:watch\""
  },
  "author": "alxlchnr",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^10.12.2",
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.7",
    "typescript": "^3.1.6"
  }
}
